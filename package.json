{
  "name": "cartographer",
  "version": "0.1.0",
  "description": "Cartographer API service",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --format esm --dts --clean",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepare": "husky"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@types/geojson": "^7946.0.16",
    "@types/node": "^20.11.0",
    "@types/pg": "^8.16.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^2.0.0",
    "tsup": "^8.0.0"
  },
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/sensible": "^6.0.4",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.4",
    "dotenv": "^17.2.3",
    "fastify": "^5.7.1",
    "kysely": "^0.28.9",
    "pg": "^8.17.1",
    "wkx": "^0.5.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
